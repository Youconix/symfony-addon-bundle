<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="darkwebdesign.symfony_addon.formtype.boolean_type" class="DarkWebDesign\SymfonyAddon\FormType\BooleanType">
            <tag name="form.type" />
        </service>

        <service id="darkwebdesign.symfony_addon.formtype.entity_type" class="DarkWebDesign\SymfonyAddon\FormType\EntityType">
            <argument type="service" id="doctrine" />
            <tag name="form.type" />
        </service>
    </services>

</container>
