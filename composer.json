{
    "name": "darkwebdesign/symfony-addon-bundle",
    "type": "library",
    "description": "Symfony Add-on Bundle",
    "keywords": ["bundle", "symfony"],
    "homepage": "http://www.darkwebdesign.nl/",
    "license": "MIT",
    "authors": [
        {"name": "Raymond Schouten"}
    ],
    "require": {
        "php": ">=8.0.2",
        "darkwebdesign/symfony-addon-pack": "6.0.*",
        "doctrine/doctrine-bundle": "*",
        "doctrine/orm": "*",
        "symfony/config": "6.0.*",
        "symfony/dependency-injection": "6.0.*",
        "symfony/http-kernel": "6.0.*"
    },
    "autoload": {
        "psr-4": {
            "DarkWebDesign\\SymfonyAddonBundle\\": "src/"
        }
